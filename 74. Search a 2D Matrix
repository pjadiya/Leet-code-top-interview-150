class Solution:
    def searchMatrix(self, matrix: List[List[int]], target: int) -> bool:
        m=len(matrix)
        n=len(matrix[0])
        i=0
        j=n-1
        
        while(i<m and j>-1):
            cur= matrix[i][j]
            if cur==target:
                return True
            elif cur<target:
                i+=1
            elif cur>target:
                j-=1
        return False
