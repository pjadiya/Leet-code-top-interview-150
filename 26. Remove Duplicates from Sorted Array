
class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        n = len(nums)
        if n==1:
            return 1

        i=1
        j=1
        max_num=nums[i-1]
        while i<n and j<n and i<=j:
            if i==j:
                j+=1
            elif nums[j]<=max_num:
                j+=1
            elif nums[i]<=max_num:
                max_num = nums[j]
                nums[i], nums[j]= nums[j], nums[i]
                i+=1
                j+=1
            else:
                max_num= nums[i]
                i+=1
        
        if (i==n-1) and nums[i]>nums[i-1]:
            return i+1
            
        return i

        
